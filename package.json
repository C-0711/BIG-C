{
  "name": "0711-c-intelligence",
  "version": "1.0.0",
  "description": "The Mother of All Agentic Enterprise Systems - OpenClaw + Enterprise UI + Data Connectors",
  "private": true,
  "scripts": {
    "dev": "pnpm --parallel dev",
    "dev:ui": "cd packages/ui && pnpm dev",
    "dev:config": "cd packages/config-ui && pnpm dev",
    "dev:integration": "cd packages/integration && pnpm dev",
    "build": "pnpm -r build",
    "start": "docker-compose up -d",
    "stop": "docker-compose down",
    "openclaw": "cd packages/core && pnpm openclaw",
    "gateway": "cd packages/core && pnpm gateway:watch",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test"
  },
  "keywords": [
    "ai",
    "enterprise",
    "openclaw",
    "agents",
    "workflows",
    "data-intelligence",
    "pim",
    "content-generation"
  ],
  "author": "0711 Intelligence",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/0711/0711-c-intelligence"
  }
}
